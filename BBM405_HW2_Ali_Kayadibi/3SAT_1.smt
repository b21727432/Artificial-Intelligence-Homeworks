(declare-const x Bool)
(declare-const y Bool) 
(declare-const z Bool)
(declare-const t Bool)
(declare-const a Bool)
(declare-const b Bool)
(assert (and
   (or x z t)
   (or y a (not b))
   (or t a b)
   (or y t a)
   (or (and (or z a) (and x b)) (or t a) )
   (or z y x)
   )
)
(check-sat)
(get-model)

(exit)